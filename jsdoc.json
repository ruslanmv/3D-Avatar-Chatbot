{
  "source": {
    "include": ["js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "encoding": "utf8"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  }
}
